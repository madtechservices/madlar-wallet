{
    "name": "tomatophp/tomato-wallet",
    "type": "library",
    "description": "Wallet Management for tomato framework",
    "keywords": [
        "php",
        "laravel",
        "template"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\TomatoWallet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\TomatoWallet\\TomatoWalletServiceProvider"
            ],
            "aliases": {
                "Payment": "TomatoPHP\\TomatoWallet\\Facades\\Payment"
            }
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "tomatophp/console-helpers": "^1.1",
        "tomatophp/tomato-admin": "^1.1",
        "tomatophp/tomato-crm": "^1.1",
        "tomatophp/tomato-locations": "^1.1",
        "bavix/laravel-wallet": "^10.0",
        "nesbot/carbon": "^2.70",
        "guzzlehttp/guzzle": "^7.8",
        "ramsey/uuid": "^4.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "squizlabs/php_codesniffer": "^3.7"
    }
}
